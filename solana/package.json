{
  "name": "xaam-solana",
  "version": "0.1.0",
  "description": "Solana programs for the XAAM (Xpress AI Agent Marketplace) protocol",
  "main": "index.js",
  "scripts": {
    "build": "cargo build-bpf --manifest-path=./programs/xaam/Cargo.toml",
    "deploy": "node scripts/deploy.js",
    "deploy:localhost": "node scripts/deploy.js localhost",
    "deploy:devnet": "node scripts/deploy.js devnet",
    "deploy:testnet": "node scripts/deploy.js testnet",
    "deploy:mainnet": "node scripts/deploy.js mainnet-beta",
    "test": "anchor test",
    "clean": "cargo clean --manifest-path=./programs/xaam/Cargo.toml"
  },
  "keywords": [
    "solana",
    "blockchain",
    "xaam",
    "marketplace",
    "nft"
  ],
  "author": "XAAM Team",
  "license": "MIT",
  "dependencies": {
    "@project-serum/anchor": "^0.24.2",
    "@solana/spl-token": "^0.3.6",
    "@solana/web3.js": "^1.73.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}